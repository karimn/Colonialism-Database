{% extends "base.html" %}

{% block main_box_container %}

<div id="inner_body_container">
    <div class="inner_top_container">
        {% if account %}
            <p>Account successfully activated</p>
            <p><a href="{% url auth_login %}">Log in</a></p>
        {% else %}
            <p>Account activation failed</p>
        {% endif %}
    </div>
</div>

{% endblock %}
